{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.2.0",
    "generators": {
      "v1.0": {
        "generatorName": "typescript-axios",
        "output": "#{cwd}/src/api/generated/v1.0/",
        "inputSpec": "https://localhost:5020/swagger/v1/swagger.json",
        "templateDir": "#{cwd}/.openapi-generator/templates",
        "additionalProperties": {
          "supportsES6": true,
          "withInterfaces": true,
          "withSeparateModelsAndApi": true,
          "apiPackage": "models",
          "modelPackage": "models"
        }
      }
    }
  }
}
